{
  "name": "build-base",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/register": "^7.8.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.1",
    "core-js": "2",
    "express": "^4.17.1",
    "fbjs": "^1.0.0",
    "ignore-styles": "^5.0.1",
    "jest-cli": "^24.9.0",
    "jest-dom": "^4.0.0",
    "jsdom": "^16.1.0",
    "lodash": "^4.17.19",
    "path": "^0.12.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-global": "^0.1.8",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "saga": "^4.0.0-alpha"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --color",
    "build": "webpack --mode production",
    "lint": "eslint --ext=js src",
    "test": "jest --verbose",
    "server:build-server": "NODE_ENV=development webpack --config webpack.server.js --mode=development -w",
    "server:start": "nodemon ./server-build/index.js",
    "server": "npm-run-all --parallel build server:*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "circleci local execute"
    }
  },
  "jest": {
    "roots": [
      "src/"
    ],
    "testRegex": "((\\.|/)(test))\\.js?$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-arrow-functions": "^7.7.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.4",
    "babel-plugin-styled-components": "^1.10.6",
    "css-loader": "^3.3.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "husky": "^4.2.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.12.0",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-node-externals": "^1.7.2"
  }
}
